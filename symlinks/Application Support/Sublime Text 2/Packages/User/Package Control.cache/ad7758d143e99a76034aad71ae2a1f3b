odacosta/sublime-php-getters-setters/master/packages.json": [{"author": "Nuno Franco da Costa", "releases": [{"date": "2013-10-22 17:17:55", "platforms": ["*"], "version": "2.0.10", "sublime_text": ">=3000", "url": "https://codeload.github.com/francodacosta/sublime-php-getters-setters/zip/2.0.10"}, {"date": "2013-09-06 00:00:00", "platforms": ["*"], "version": "1.0.10", "sublime_text": "<3000", "url": "https://codeload.github.com/francodacosta/sublime-php-getters-setters/zip/1.0.10"}], "homepage": "https://github.com/francodacosta/sublime-php-getters-setters", "labels": ["auto-complete", "php", "snippets", "code generation", "text manipulation", "formatting"], "buy": null, "previous_names": [], "name": "PHP Getters and Setters", "donate": "https://www.gittip.com/on/github/francodacosta/", "issues": "https://github.com/francodacosta/sublime-php-getters-setters/issues", "description": "generare getters and setters for php classes", "readme": "https://raw.github.com/francodacosta/sublime-php-getters-setters/master/Readme.md"}], "https://raw.github.com/merlinthered/sublime-rainmeter/master/packages.json": [{"author": "merlinthered", "releases": [{"date": "2014-01-17 20:25:00", "platforms": ["windows"], "version": "1.0.7", "sublime_text": "<3000", "url": "https://codeload.github.com/merlinthered/sublime-rainmeter/zip/1.0.7"}], "homepage": "http://merlinthered.github.com/sublime-rainmeter/", "labels": [], "buy": null, "previous_names": [], "name": "Rainmeter", "donate": null, "issues": null, "description": "Rainmeter Package for Sublime Text 2", "readme": null}], "https://raw.github.com/ikeike443/Sublime-Scalariform/master/packages.json": [{"author": "ikeike443", "releases": [{"date": "2013-04-14 23:50:00", "platforms": ["*"], "version": "0.3", "sublime_text": "<3000", "url": "https://codeload.github.com/ikeike443/Sublime-Scalariform/zip/0.3"}], "homepage": "https://github.com/ikeike443/Sublime-Scalariform", "labels": [], "buy": null, "previous_names": [], "name": "Scalar