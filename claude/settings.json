{
  "respectGitignore": true,
  "permissions": {
    "allow": [
      "Read",
      "Edit",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git branch:*)",
      "Bash(git show:*)",
      "Bash(git stash:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git checkout:*)",
      "Bash(git switch:*)",
      "Bash(git fetch:*)",
      "Bash(git pull:*)",
      "Bash(git merge:*)",
      "Bash(git remote:*)",
      "Bash(gh:*)",
      "Bash(bundle exec:*)",
      "Bash(bundle install:*)",
      "Bash(bundle update:*)",
      "Bash(rake:*)",
      "Bash(ruby:*)",
      "Bash(node:*)",
      "Bash(npm run:*)",
      "Bash(npm test:*)",
      "Bash(npm install:*)",
      "Bash(yarn:*)",
      "Bash(mise:*)",
      "Bash(docker:*)",
      "Bash(docker compose:*)",
      "Bash(ls:*)",
      "Bash(which:*)",
      "Bash(pwd)",
      "Bash(env)",
      "Bash(mkdir:*)",
      "Bash(wc:*)",
      "Bash(sort:*)",
      "Bash(rg:*)",
      "Bash(fd:*)",
      "Bash(bat:*)",
      "Bash(jq:*)",
      "Bash(yq:*)"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Read(.env)",
      "Read(.env.*)"
    ]
  },
  "model": "opus",
  "enabledPlugins": {
    "superpowers@claude-plugins-official": true,
    "github@claude-plugins-official": true
  }
}
